import java.util.LinkedList;
import java.util.Scanner;

//Lists each prime until asked to stop
public class NextPrime {

	public static void main(String[] args) {
		
		System.out.println("This program will list primes until receiving the input -1."
				+ "\n" + "Give it any other integer value for it to go to the next prime.");
		
		LinkedList<Integer> capturedPrimes = new LinkedList<Integer>();
		
		Scanner sc = new Scanner(System.in);
		
		while(true) {
			
			int n = sc.nextInt();
			
			if (n == -1) {
				break;
			}
			
			
			
		}
		
		sc.close();
		
	}
	
	static boolean checkPrime(int checkedPrime){
		for (int i = 2; i < Math.sqrt(checkedPrime); i++) {
			if (checkedPrime % i == 0) {
				return false;
			}
		}
		return true;
	}
	
}
