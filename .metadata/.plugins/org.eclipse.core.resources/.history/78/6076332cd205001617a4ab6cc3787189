import java.util.LinkedList;
import java.util.Scanner;

//Lists each prime until asked to stop
public class NextPrime {

	static LinkedList<Integer> capturedPrimes = new LinkedList<Integer>();
	
	public static void main(String[] args) {
		
		System.out.println("This program will list primes until receiving the input -1."
				+ "\n" + "Give it any other integer value for it to go to the next prime.");
		
		Scanner sc = new Scanner(System.in);
		
		capturedPrimes.add(1);
		capturedPrimes.add(2);
		
		int current = 3;
		
		while(true) {
			
			int n = sc.nextInt();
			
			if (n == -1) {
				break;
			}
			
			returnPrime(current);
			
			current++;
		}
		
		sc.close();
		
	}
	
	static void returnPrime(int max) {
		for (int i = 2; i <= max; i++) {
			if (!capturedPrimes.contains(i)) {
				if (checkPrime(i)) {
					System.out.printf("%d ", i);
	
					capturedPrimes.add(i);	
				}
			}
		}
	}
	
	static boolean checkPrime(int checkedPrime){
		for (int j = 2; j < Math.sqrt(checkedPrime); j++) {
			if (checkedPrime % j == 0) {
				return false;
			}
		}
		return true;
	}
	
}
